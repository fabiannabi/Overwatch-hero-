-right: auto;\r\n  margin-left: auto;\r\n  padding: 1.5em;\r\n  width: 25em;\r\n  border-radius: 1.5em;\r\n  margin-top: 45em;\r\n  font-size: 1.1em;\r\n  font-weight: bold;\r\n  text-transform: uppercase;\r\n  border-color: #555;\r\n  background-color: #f99e1a;\r\n  transition-timing-function: ease-in;\r\n  transition: 0.5s;\r\n  color: rgba(15, 14, 14, 0.781);\r\n}\r\nbutton:hover {\r\n  background-color: #f9a01ab6;\r\n  transition-timing-function: ease-out;\r\n  transition: 0.5s;\r\n  color: rgba(39, 38, 38, 0.911);\r\n}\r\n.main-nav {\r\n  border-bottom: transparent;\r\n}\r\n.options {\r\n  display: flex;\r\n  justify-content: space-evenly;\r\n}\r\n\r\n.options li {\r\n  display: block;\r\n  font-weight: bold;\r\n  font-size: 1.5em;\r\n  text-align: center;\r\n  padding: 0.7em;\r\n}\r\n.options li a {\r\n  color: white;\r\n  text-decoration: none;\r\n}\r\n\r\n.options li a:hover {\r\n  color: rgb(226, 157, 30);\r\n  text-decoration: none;\r\n}\r\n/*SECTION 1 HEROES ASSAULT*/\r\n\r\n.hero-tittle {\r\n  background-color: #626d74;\r\n  padding: 1.5em;\r\n  text-align: center;\r\n  font-size: 1.8em;\r\n  font-weight: bold;\r\n  text-transform: uppercase;\r\n  color: #ac7b31;\r\n}\r\n.thumbnails {\r\n  display: flex;\r\n  justify-content: space-evenly;\r\n}\r\n\r\n.thumbnails img {\r\n  height: auto;\r\n  margin: 0 0.5em;\r\n  clip-path: polygon(25% 0%, 100% 0%, 75% 100%, 0% 100%);\r\n}\r\n.hog:hover,\r\n.soldier:hover,\r\n.genji:hover,\r\n.Dva:hover,\r\n.mercy:hover,\r\n.tracer:hover {\r\n  transform: scale(1.01);\r\n  display: block;\r\n}\r\n\r\n.hero-gallery {\r\n  display: flex;\r\n  align-items: center;\r\n  justify-content: space-between;\r\n}\r\n\r\n.hero-description {\r\n  font-size: 1.8em;\r\n  text-transform: uppercase;\r\n  color: black;\r\n  text-align: justify;\r\n  padding: 2em;\r\n}\r\n\r\n.hero-image {\r\n  min-width: 45em;\r\n  padding: 1.5em;\r\n  display: flex;\r\n  justify-content: center;\r\n}\r\n\r\n.hero-picture {\r\n  max-width: 650px;\r\n}\r\n\r\n/*SECTION 3 MAPS*/\r\n.maps {\r\n  padding: 1.5em;\r\n  text-align: center;\r\n  font-size: 1.8em;\r\n  font-weight: bold;\r\n  text-transform: uppercase;\r\n  color: #d68a18;\r\n}\r\n\r\n.map-dorado img {\r\n  width: 70%;\r\n}\r\n\r\nselect {\r\n  font-size: 1.5em;\r\n  background-color: #ece0cc;\r\n}\r\ninput {\r\n  font-size: 1.5em;\r\n  background-color: #d68a18;\r\n  color: #223040;\r\n}\r\n\r\n.maps-input {\r\n  display: block;\r\n  justify-content: center;\r\n  margin: 1em 1em;\r\n}\r\n\r\n.map-image {\r\n  max-width: 1000px;\r\n}\r\n\r\n.maps-types {\r\n  padding: 1em;\r\n  display: flex;\r\n  color: black;\r\n  font-size: 1.8em;\r\n  align-items: center;\r\n  justify-content: space-evenly;\r\n}\r\n.map-description {\r\n  margin: 0.5em;\r\n  width: 16%;\r\n}\r\n\r\n/*SECTION 3 League*/\r\n\r\n.league-teams {\r\n  margin-top: 1em;\r\n  font-size: 5em;\r\n  color: rgb(0, 0, 0);\r\n  text-align: center;\r\n  display: flex;\r\n  align-items: center;\r\n  justify-content: space-evenly;\r\n}\r\n\r\n.shanghai,\r\n.fili,\r\n.fuel,\r\n.seoul,\r\n.outlaws {\r\n  max-width: 250px;\r\n}\r\n.shanghai:hover,\r\n.fili:hover,\r\n.fuel:hover,\r\n.seoul:hover,\r\n.outlaws:hover {\r\n  transform: scale(1.01);\r\n  display: block;\r\n}\r\n\r\n.team-logo {\r\n  max-width: 1000px;\r\n  margin-left: 0.5em;\r\n}\r\n\r\n.thumbnails-team {\r\n  display: flex;\r\n  justify-content: space-evenly;\r\n}\r\n.team-score {\r\n  width: 50%;\r\n}\r\n"},"lineCount":null}},"error":null,"hash":"794b9c2bd79a09cec854f6fbda6976ce","cacheData":{"env":{}}}