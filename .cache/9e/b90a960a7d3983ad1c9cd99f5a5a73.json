ox-sizing: border-box;\r\n  margin: 0;\r\n  padding: 0;\r\n}\r\n\r\nbody,\r\nhtml {\r\n  font-family: \"Raleway\", sans-serif;\r\n  background-color: #223040;\r\n  color: #555;\r\n}\r\n\r\n/*HEADER*/\r\nheader {\r\n  background-image: url(img/logo-hero.jpg);\r\n  height: 100vh;\r\n  background-size: cover;\r\n  background-position: center;\r\n}\r\nbutton {\r\n  display: flex;\r\n  justify-content: center;\r\n  margin-right: auto;\r\n  margin-left: auto;\r\n  padding: 1.5em;\r\n  width: 25em;\r\n  border-radius: 1.5em;\r\n  margin-top: 45em;\r\n  font-size: 1.1em;\r\n  font-weight: bold;\r\n  text-transform: uppercase;\r\n  border-color: #555;\r\n  background-color: #f99e1a;\r\n  transition-timing-function: ease-in;\r\n  transition: 0.5s;\r\n  color: rgba(15, 14, 14, 0.781);\r\n}\r\nbutton:hover {\r\n  background-color: #f9a01ab6;\r\n  transition-timing-function: ease-out;\r\n  transition: 0.5s;\r\n  color: rgba(39, 38, 38, 0.911);\r\n}\r\n.main-nav {\r\n  border-bottom: transparent;\r\n}\r\n.options {\r\n  display: flex;\r\n  justify-content: space-evenly;\r\n}\r\n\r\n.options li {\r\n  display: block;\r\n  font-weight: bold;\r\n  font-size: 1.5em;\r\n  text-align: center;\r\n  padding: 0.7em;\r\n}\r\n.options li a {\r\n  color: white;\r\n  text-decoration: none;\r\n}\r\n\r\n.options li a:hover {\r\n  color: rgb(241, 139, 56);\r\n  text-decoration: none;\r\n}\r\n/*SECTION 1 HEROES ASSAULT*/\r\n.hero-assasin,\r\n.hero-tank,\r\n.hero-support {\r\n  background-color: #425974f8;\r\n}\r\n\r\n.hero-class {\r\n  background-color: #5f7d8d;\r\n  padding: 1.5em;\r\n  text-align: center;\r\n  font-size: 1.8em;\r\n  font-weight: bold;\r\n  text-transform: uppercase;\r\n  color: #d68a18;\r\n}\r\n.thumbnails {\r\n  display: flex;\r\n  justify-content: space-around;\r\n}\r\n\r\n.thumbnails img {\r\n  height: auto;\r\n  margin: 0 0.5em;\r\n  clip-path: polygon(25% 0%, 100% 0%, 75% 100%, 0% 100%);\r\n}\r\n\r\n.tracer:hover {\r\n  transform: scale(1.05);\r\n  display: block;\r\n}\r\n\r\n.hero-gallery {\r\n  display: flex;\r\n  align-items: center;\r\n  justify-content: space-between;\r\n}\r\n\r\n.hero-description {\r\n  font-size: 2em;\r\n  text-transform: uppercase;\r\n  color: black;\r\n}\r\n\r\n.hero-image {\r\n  min-width: 45em;\r\n  padding: 2em;\r\n  display: flex;\r\n  justify-content: center;\r\n}\r\n\r\n/*SECTION 2 HEROES TANK*/\r\n\r\n.Dva:hover {\r\n  transform: scale(1.05);\r\n  display: block;\r\n}\r\n\r\n.hero-gallery-Dva {\r\n  display: flex;\r\n  align-items: center;\r\n}\r\n\r\n.Dva-picture {\r\n  width: 100%;\r\n}\r\n\r\n/*SECTION 2 HEROES SUPPORT*/\r\n\r\n.mercy:hover {\r\n  transform: scale(1.05);\r\n  display: block;\r\n}\r\n\r\n.hero-gallery mercy {\r\n  display: flex;\r\n  align-items: center;\r\n}\r\n\r\n.mercy-picture {\r\n  width: 100%;\r\n}\r\n\r\n/*SECTION 3 MAPS*/\r\n.maps {\r\n  background-color: #5f7d8d;\r\n  padding: 1.5em;\r\n  text-align: center;\r\n  font-size: 1.8em;\r\n  font-weight: bold;\r\n  text-transform: uppercase;\r\n  color: #d68a18;\r\n}\r\n\r\n.maps-description {\r\n  background-color: #5f7d8d;\r\n}\r\n\r\ntable,\r\nth,\r\ntd {\r\n  border: 0.1em solid rgb(142, 169, 177);\r\n}\r\n\r\ntd {\r\n}\r\n\r\n.map-dorado img {\r\n  width: 70%;\r\n}\r\n"},"lineCount":null}},"error":null,"hash":"6cdcaf68bbe64fe32696525e6c82cc48","cacheData":{"env":{}}}